{% extends 'base.html.twig' %}

{% block title %}Catégorie de service{% endblock %}

{% block body %}
    <h1 class="text-center">Catégorie de service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>N°</th>
                <td>{{ catservice.catid }}</td>
            </tr>
            <tr>
                <th>Libelé de la Catégorie de Service</th>
                <td>{{ catservice.libcat }}</td>
            </tr>
            <tr>
                <th>Code Libelé de la Catégorie de Service</th>
                <td>{{ catservice.catcode }}</td>
            </tr>
            <tr>
                <th>Type Unité</th>
                <td>{{ catservice.typeid }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('catservice_index') }}" class="btn btn-secondary btn-sm">retour à la liste</a>

    <a href="{{ path('catservice_edit', {'catid': catservice.catid}) }}" class="btn">Modifier</a>

    {{ include('catservice/_delete_form.html.twig') }}
{% endblock %}
