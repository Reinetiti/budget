{% extends 'base.html.twig' %}

{% block title %}Mission budgétaire{% endblock %}

{% block body %}
    <h1 class="text-center">Affichage de la Mission budgétaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>N°</th>
                <td>{{ misbudget.misbudgetid }}</td>
            </tr>
            <tr>
                <th>Libelé de la mission budgetaire</th>
                <td>{{ misbudget.libmisbudget }}</td>
            </tr>
            <tr>
                <th>Code de la mission budgetaire</th>
                <td>{{ misbudget.misbudgetcode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('misbudget_index') }}">retour à la liste</a>

    <a href="{{ path('misbudget_edit', {'misbudgetid': misbudget.misbudgetid}) }}">modifier</a>

    {{ include('misbudget/_delete_form.html.twig') }}
{% endblock %}
