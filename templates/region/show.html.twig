{% extends 'base.html.twig' %}

{% block title %}Région{% endblock %}

{% block body %}
    <h1>Région</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>N°</th>
                <td>{{ region.regionid }}</td>
            </tr>
            <tr>
                <th>Libelé de la région</th>
                <td>{{ region.libregion }}</td>
            </tr>
            <tr>
                <th>Code de la région</th>
                <td>{{ region.regioncode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('region_index') }}">retour à la liste</a>

    <a href="{{ path('region_edit', {'regionid': region.regionid}) }}">modifier</a>

    {{ include('region/_delete_form.html.twig') }}
{% endblock %}
