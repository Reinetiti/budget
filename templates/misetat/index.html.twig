{% extends 'base.html.twig' %}

{% block title %}Misetat index{% endblock %}

{% block body %}
    <h1>Misetat index</h1>

    <a href="{{ path('misetat_new') }}" class="btn">Ajouter</a>
    <table class="table">
        <thead>
            <tr>
                <th>N°</th>
                <th>Libelé de la mission d'Etat</th>
                <th>Code de la mission d'Etat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for misetat in misetats %}
            <tr>
                <td>{{ misetat.misetatid }}</td>
                <td>{{ misetat.libmisetat }}</td>
                <td>{{ misetat.misetatcode }}</td>
                <td>
                    <a href="{{ path('misetat_show', {'misetatid': misetat.misetatid}) }}">Afficher</a>
                    <a href="{{ path('misetat_edit', {'misetatid': misetat.misetatid}) }}">Modifier</a>
                    <a href="{{ path('misetat_delete', {'misetatid': misetat.misetatid}) }}">Supprimer</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {# <a href="{{ path('misetat_new') }}">Ajouter</a> #}
{% endblock %}
